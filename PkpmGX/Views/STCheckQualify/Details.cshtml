                            <div class="layui-input-block">
                                <input type="text" name="businesspermit" value="@(Model.myT_bp_Custom.businesspermit)" placeholder="" autocomplete="off" class="layui-input">
                            </div>
                        </div>
                    </div>
                    <div class="layui-form-item">
                        <div class="layui-col-md6">
