                                <input type="text" name="id" lay-verify="required" value="@Model.myT_bp_Custom.Id" placeholder="" autocomplete="off" class="layui-input">
                            </div>
                        </div>
                    </div>
                    <div class="layui-form-item">
                        <div class="layui-col-md6">
                            <div class="layui-form-label">所在区域<span style="color:red">*</span></div>
