@model PkpmGX.Models.STPumpvehicleModel

@{
    ViewBag.Title = "Pumpvehicle";
    Layout = "~/Views/Shared/LayuiTab_Layout.cshtml";
}

 
<div class="layui-container">
    <div class="layui-row" id="STQualify-PumpSystem-Container">
        <form class="layui-form" action="" lay-filter="STQualify-PumpSystem-form">
            <div class="layui-card-body">
                <table class="layui-table">
                    <colgroup>
                        <col width="200">
                        <col width="200">
                        <col>
                    </colgroup>
                    <thead>
                        <tr>
                            <th>种类</th>
                            <th>高度</th>
                            <th>数量</th>
                             
                        </tr>
                    </thead>
                    <tbody>
                        @{
                            foreach (var item in Model.Pumpvehicles)
                            {
                                <tr><td>@item.variety</td><td>@item.spec</td><td>@item.num</td></tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
        </form>
    </div>
</div>

