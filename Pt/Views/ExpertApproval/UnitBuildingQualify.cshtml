                            <tr>
                                <td colspan="7" align="center">（建设工程质量检测机构资质）</td>
                            </tr>
                            <tr>
                                <td width="8%">申请单位</td>
                                <td>
                                    <input type="text" name="unitName" id="unitName" value="@Model.UnitName" readonly placeholder="" autocomplete="off" class="layui-input">
                                    <input type="text" name="pid" id="pid" value="@Model.ID" placeholder="" autocomplete="off" class="layui-hide">
                                </td>
